{
  "name": "functions",
  "scripts": {
    "deploy:1G": "gcloud functions deploy obtainSiteMetadata --runtime nodejs10 --trigger-http --memory=1024MB",
    "deploy:obtainSiteMetadata": "gcloud functions deploy obtainSiteMetadata --runtime nodejs10 --trigger-http",
    "deploy:screenshotDomain": "gcloud functions deploy screenshotDomain --runtime nodejs10 --trigger-http --memory=2048MB",
    "lint": "eslint . --fix -c .eslintrc.js --ext js,ts --ignore-pattern='!.*'",
    "build": "tsc",
    "dev": "tsc -w",
    "hello": "functions-framework --target=helloWorld",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "_deploy": "firebase deploy --only functions",
    "shot": "ts-node examples/shot.ts",
    "cap": "ts-node cap.ts",
    "ogp": "ts-node ogp.ts",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "node-fetch": "^2.6.0",
    "puppeteer": "^5.2.1",
    "request": "^2.88.2",
    "unfurl.js": "^5.1.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/open-graph-scraper": "^4.3.0",
    "@types/puppeteer": "^3.0.1",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "npm-watch": "^0.6.0",
    "playwright": "^1.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
